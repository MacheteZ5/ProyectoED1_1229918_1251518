
@{
    ViewBag.Title = "Crear Tabla";
}
@{
    ViewBag.Title = "Conección";
}
<style>

    textarea {
        border: 1px solid white;
        background: #50778f;
        color: white;
        padding: 10px;
        font-size: larger;
    }

    #boton {
        background-color: #003366;
        border: none;
        color: white;
        padding: 8px 16px;
        text-decoration: none;
        margin: 4px 2px;
        cursor: pointer;
        text-align: center;
        font-weight: bold;
    }

    h1 {
        color: white;
        font-weight: bold;
    }

    body {
        background-image: url(https://downloadwallpaper.org/wp-content/uploads/2016/11/cool-wallpaper-backgrounds-HD7.jpg);
    }
</style>
<script>
    function goBack() {
        window.history.back();
    }
    function notificar() {
        if (Notification.permission !== "granted")
            Notification.requestPermission();
        else {
            var notification = new Notification('AVISO SOBRE CREACIÓN', {
                icon: 'data:image/png;base64,',
                body: "La tabla se creó exitosamente",
            });

            notification.onclick = function () {
                document.getElementById("modificar").submit();
            };
        }
    }
</script>
<hr />
<div class="col-md-12">
    <center>
        <h1>Ingrese la sentencia para la creación de la tabla</h1>
        <form action="CreaciónTabla2" method="post">
            <textarea rows="6" cols="50" name="SQLs">
            </textarea><br />
            <button id="boton" onclick="notificar()">Crear Tabla</button>
        </form>
        <button id="boton" onclick="goBack()">Volver</button>
    </center>
</div>